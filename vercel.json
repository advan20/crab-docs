{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    {"source": "/connect/(.*)", "destination": "/api/crab/grant"},
    {"source": "/pangolin/(.*)","destination": "/api/pangolin/grant"}
  ]
}
