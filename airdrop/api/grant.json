{
  "defaults": {
    "origin": "https://docs.crab.network",
    "transport": "querystring",
    "state": true
  },
  "github": {
    "key": "{{ github_oauth_app_key }}",
    "secret": "{{ github_oauth_app_secret }}",
    "scope": [],
    "callback": "/api/crab/authorization",
    "overrides": {
      "crab": {
        "callback": "/api/crab/authorization"
      },
      "pangolin": {
        "callback": "/api/pangolin/authorization"
      }
    }
  }
}
